
<% content_for(:title, t('.title')) %>
<div class="max1280-container">
  <div>
    <h1 class="section-title" style="margin-top: 2rem;">
      <span class="red-text">新規作成</span>
    </h1>

    <%= form_with model: @item, id: 'item_create_form', multipart: true, data: { turbo: false }, html: { onsubmit: 'return false;' } do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.hidden_field :sub_user_id, value: @sub_user.id %>
      <%= render 'form_item_create', f: f %>
      
      <div class="user-createform">
      
        <%= f.submit "作成", class: "btn btn-primary", id: "submit-button", type: 'button' %>
      </div>
    <% end %>
  </div> 
</div>





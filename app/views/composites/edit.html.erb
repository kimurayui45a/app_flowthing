<% content_for(:title, t('.title')) %>
<div class="p5canvas-container">
  <h1 class="section-title" style="margin-top: 2rem;">
    <span class="red-text">Flux Screen編集</span>
  </h1>

  <div id="reactEditComposite"
    data-profile-id="<%= @profile.id %>"
    data-composite-id="<%= @composite.id %>"
    data-item-id-all="<%= @items.to_json %>"
    data-space-id-all="<%= @spaces.to_json %>"
    data-sub-user-id-all="<%= @sub_users.to_json %>"
    data-space-data="<%= @composite.composite_space %>"
    data-item-data="<%= @composite.composite_item %>"

    data-composite-name="<%= @composite.composite_name %>"
    data-composite-text="<%= @composite.composite_text %>"
    data-composite-image="<%= @composite.composite_image %>"
    data-composite-space-id="<%= @composite.space_id %>"
    data-space-size="<%= @profile.space_size %>"
  >
  </div>
</div>

